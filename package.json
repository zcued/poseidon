  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.2.0",
    "@babel/core": "^7.2.0",
    "@commitlint/cli": "^7.2.1",
    "@commitlint/config-conventional": "^7.1.2",
    "@types/react": "^16.7.13",
    "babel-eslint": "7.2.3",
    "babel-preset-next": "^1.2.0",
    "commitizen": "^3.0.5",
    "cz-lerna-changelog": "^2.0.1",
    "enzyme": "^3.8.0",
    "enzyme-adapter-react-16": "^1.7.1",
    "enzyme-to-json": "^3.3.5",
    "eslint": "^5.10.0",
    "eslint-config-zcool": "^1.5.0",
    "husky": "^1.0.0-rc.13",
    "jest": "^23.4.2",
