{
  "compilerOptions": {
    "target": "es6",
    "module": "commonjs",
    "jsx": "react",
    "moduleResolution": "node",
    "noUnusedLocals": false,
    "noUnusedParameters": true,
    "removeComments": false,
    "preserveConstEnums": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "baseUrl": "/",
    "declaration": true,
    "lib": ["es6", "es2017", "dom"]
  }
}
